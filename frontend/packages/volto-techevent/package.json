{
  "name": "@plone-collective/volto-techevent",
  "version": "1.0.0-alpha.3",
  "description": "Support for implementing tech conference in a Plone site.",
  "main": "src/index.ts",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "PloneGov-BR",
  "homepage": "https://github.com/collective/tech-event#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:collective/tech-event"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
